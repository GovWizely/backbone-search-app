{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "spread": true
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react",
    "mocha"
  ],
  "rules": {
    "comma-dangle": [2, "never"],
    "global-require": 0,
    "indent": [2, 2],
    "jsx-curly-spacing": [2, "always"],
    "no-underscore-dangle": 0,
    "quote-props": [2, "consistent-as-needed"],
    "react/sort-prop-types": 1
  }
}
